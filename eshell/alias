alias ls ls --color --group-directories-first $1
alias ll ls -alh $1
alias l ll $*
alias pd pwd
alias d dired $1
alias ee find-file-other-window $1
alias ff find-file $1
alias mkdir mkdir -p $1
alias du du -sh * | sort -h
alias ..... cd ../../../..
alias .... cd ../../..
alias ... cd ../..
alias .. cd ..

